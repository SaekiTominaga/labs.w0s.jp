{
	"root": true,
	"env": {
		"browser": true,
		"es2022": true
	},
	"extends": ["@w0s/eslint-config"],
	"parserOptions": {
		"sourceType": "module"
	},
	"plugins": ["html"],
	"rules": {
		"no-console": "off"
	},
	"overrides": [
		{
			"files": ["./public/http/reporting/*.html"],
			"rules": {
				"no-alert": "off"
			}
		}
	]
}
